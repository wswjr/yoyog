/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.shop-fl{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction:row;
}
navigator{
  display: inline;
}
.shop-fl view{
  flex: 1;
  font-size: 28rpx;
  display:flex;
  align-items:center;
  justify-content:center;

}
.shop-fl view image{
    width: 35rpx;
    height: 35rpx;
    margin-left:6rpx;
}
.images-wh{
  width: 100%;
  height: 100%;
}
page{
   height: 100%;
   background-color: #ededed;
   }

/* 箭头 */
.orders-no-address {
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  color: #090909;
  border-bottom: 2rpx solid #ededed;
  text-align: center;
  background-color: #fff;
}
.list {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 30rpx 0;
}
.orders-no-address::after {
  position: absolute;
  left: 30rpx;
  top: 34rpx;
  content: '';
  width: 16rpx;
  height: 16rpx;
  border-top: 4rpx solid #7f7f7f;
  border-left: 4rpx solid #7f7f7f;
  -webkit-transform: rotate(45deg);
  transform: rotate(320deg);
}